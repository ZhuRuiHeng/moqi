/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /*justify-content: space-between;*/
  box-sizing: border-box;
  color: #333;
} 

button {
	position: static;
	display: inline-block;
	margin-left: 0;
	margin-right: 0;
	padding-left: 0;
	padding-right: 0;
	box-sizing: content-box;
	font-size: inherit;
	text-align: center;
	text-decoration: none;
	line-height: 1;
	border-radius: 0;
	-webkit-tap-highlight-color: transparent;
	overflow: hidden;
	color: transparent;
	background-color: transparent;
}

button:after {
	display: none;
}



.home_icon{
  width:70rpx;
  height:70rpx;
  background: rgba(0,0,0,0.3);
  border-radius:50%;
  display:flex;
  justify-content:center;
  align-items:center;
  position:fixed;
  right:30rpx;
  top: 30rpx;
  padding: 0;
  z-index: 10;
}
.home_icon image{
  display:block;
  width:44rpx;
  height:42rpx;
}